<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hillssoft.app"
    android:versionCode="16"
    android:versionName="1.3" >

    <uses-sdk
        android:minSdkVersion="5"
        android:targetSdkVersion="17" />


    
    <!-- Permission -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="com.hillssoft.app.StaticBroadcastReceiver" />
        
    
    
    
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        
        
        
        
        
        <!-- Broadcast Receiver -->
        <receiver
            android:name=".sample.receiver.StartReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        
        <receiver 
            android:name=".sample.receiver.AlertReceiver" >
            <intent-filter>
                <action android:name="StaticBroadcastReceiver" />
            </intent-filter>
        </receiver>
        
        
        
        
        
        <!-- Service -->
        
        
        
        
        
        
        
        
        <!-- Index Activity -->
        <activity
            android:name="com.hillssoft.app.IndexActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        
        
        <!-- Tab Activity -->
        <activity
            android:name="com.hillssoft.app.sample.SampleTabActivity"
            android:label="@string/sample_sample_tab_activity_title" >
        </activity>
        <activity
            android:name="com.hillssoft.app.mtom.MtomTabActivity"
            android:label="@string/mtom_mtom_tab_activity_title" >
        </activity>
        
        
        
        
        <!-- Sample -->
        <activity
            android:name=".sample.activity.MainActivity"
            android:label="@string/sample_activity_main_activity_title" >
        </activity>
        
        <activity
            android:name=".sample.activity.SubActivity"
            android:label="@string/sample_activity_sub_activity_title" >
        </activity>
        
        <activity
            android:name=".sample.view.ViewObjectActivity"
            android:label="@string/sample_view_view_object_activity" >
        </activity>
        
        <activity
            android:name=".sample.view.ListViewActivity"
            android:label="@string/sample_view_list_view_activity_title" >
		</activity>
        
        <activity
            android:name=".sample.view.ListViewDetailActivity"
            android:label="@string/sample_view_list_view_detail_activity_title" >
        </activity>
        
        <activity
            android:name=".sample.graphic.GraphActivity"
            android:label="@string/sample_graphic_graph_activity_title" >
        </activity>
        
        <activity
            android:name=".sample.broadcast.BroadcastActivity"
            android:label="@string/sample_broadcast_broadcast_activity_title" >
        </activity>
        
        <activity
            android:name=".sample.service.ServiceActivity"
            android:label="@string/sample_service_activity_title" >
        </activity>
        
        <activity
            android:name=".sample.db.DBActivity"
            android:label="@string/sample_db_db_activity_title" >
        </activity>
        
        
        
        
        
        
        
        
        
        
        
    </application>

</manifest>